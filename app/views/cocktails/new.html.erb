<div class="container">
  <h1>Create a cocktail</h1>

  <%= simple_form_for(@cocktail, html: { class: 'form-horizontal' }) do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.button :submit %>
  <% end %>

</div>


